    "@playwright/test": "^1.50.1",
    "@types/node": "18.19.75",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "@vitest/coverage-v8": "^3.0.5",
    "autoprefixer": "^10.4.16",
    "happy-dom": "^15.11.7",
    "postcss": "^8.5.2",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "5.7.3",
