{
  "name": "@jetstreamgg/webapp",
  "type": "module",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "dev": "vite --host --config vite.config.ts",
    "dev:restricted": "VITE_RESTRICTED_BUILD=true vite --host --config vite.config.ts",
    "dev:restricted-mica": "VITE_RESTRICTED_BUILD_MICA=true vite --host --config vite.config.ts",
    "build:restricted": "VITE_RESTRICTED_BUILD=true pnpm build",
    "build:restricted-mica": "VITE_RESTRICTED_BUILD_MICA=true pnpm build",
    "dev:mock": "VITE_USE_MOCK_WALLET=true pnpm dev",
    "build": "vite build --config vite.config.ts",
    "build:mock": "VITE_USE_MOCK_WALLET=true pnpm build",
    "preview": "pnpm build && vite preview  --config vite.config.ts",
    "preview:restricted": "VITE_RESTRICTED_BUILD=true pnpm preview",
    "preview:restricted-mica": "VITE_RESTRICTED_BUILD_MICA=true pnpm preview",
    "start": "vite --host --config vite.config.ts",
    "lint-no-files": "eslint --fix --ext .tsx,.ts,.js,.jsx",
    "lint": "pnpm lint-no-files src scripts",
    "test": "vitest run",
    "e2e": "ACCOUNT_COUNT=10 playwright test --config playwright-parallel.config.ts --workers=1 --trace on",
    "e2e:ui": "ACCOUNT_COUNT=10 playwright test --config playwright-parallel.config.ts --workers=1 --ui",
    "e2e:parallel": "playwright test --config playwright-parallel.config.ts",
    "e2e:parallel:alternate:fork": "FUND_NETWORKS=mainnet USE_ALTERNATE_VNET=true playwright test --config playwright-parallel.config.ts --project=chromium-alternate",
    "e2e:parallel:standard": "playwright test --config playwright-parallel.config.ts --project=chromium",
    "e2e:parallel:retry-failed": "SKIP_FUNDING=true playwright test --last-failed --config playwright-parallel.config.ts",
    "e2e:parallel:retry-serial": "SKIP_FUNDING=true playwright test --last-failed --workers=1 --config playwright-parallel.config.ts",
    "e2e:parallel:debug": "DEBUG=pw:api playwright test --config playwright-parallel.config.ts",
    "e2e:parallel:mainnet": "FUND_NETWORKS=mainnet playwright test --config playwright-parallel.config.ts",
    "e2e:parallel:base": "FUND_NETWORKS=base playwright test --config playwright-parallel.config.ts",
    "e2e:parallel:arbitrum": "FUND_NETWORKS=arbitrum playwright test --config playwright-parallel.config.ts",
    "e2e:parallel:optimism": "FUND_NETWORKS=optimism playwright test --config playwright-parallel.config.ts",
    "e2e:parallel:skip": "SKIP_FUNDING=true playwright test --config playwright-parallel.config.ts",
    "e2e:parallel:dev": "SKIP_FUNDING=true FUND_NETWORKS=mainnet playwright test --config playwright-parallel.config.ts",
    "e2e:fund": "npx tsx src/test/e2e/fund-accounts.ts",
    "e2e:fund:check": "SKIP_FUNDING=true npx tsx src/test/e2e/fund-accounts.ts",
    "e2e:validate-vnets": "npx tsx src/test/e2e/validate-vnets.ts",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit --project tsconfig.production.json",
    "sync-content": "./scripts/sync-content.sh",
    "bump-content-version": "./scripts/bump-content-version.sh"
  },
  "dependencies": {
    "@base-org/account": "catalog:",
