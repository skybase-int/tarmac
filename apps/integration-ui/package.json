{
  "name": "@jetstreamgg/integration-ui",
  "version": "0.1.4",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite --host --config vite.config.ts",
    "dev:restricted": "VITE_PUBLIC_RESTRICTED_BUILD=true vite --host --config vite.config.ts",
    "build": "tsc --project tsconfig.json && vite build --config vite.config.ts",
    "build:restricted": "VITE_PUBLIC_RESTRICTED_BUILD=true pnpm build",
    "preview": "pnpm build && vite preview  --config vite.config.ts",
    "preview:restricted": "VITE_PUBLIC_RESTRICTED_BUILD=true pnpm preview",
    "start": "vite --host --config vite.config.ts",
    "lint": "eslint --ext .tsx,.ts src/pages/ src/modules/",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@fontsource/plus-jakarta-sans": "^4.5.11",
    "@jetstreamgg/hooks": "workspace:^",
    "@jetstreamgg/utils": "workspace:^",
    "@jetstreamgg/widgets": "workspace:^",
    "@radix-ui/react-slot": "^1.2.0",
    "@rainbow-me/rainbowkit": "2.2.4",
    "@types/react": "19.1.2",
    "@types/react-dom": "19.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.0",
    "framer-motion": "^11.18.2",
    "lucide-react": "^0.503.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^6.30.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "peerDependencies": {
    "@lingui/detect-locale": "^5.3.1",
    "@lingui/macro": "^5.3.1",
    "@lingui/react": "^5.3.1"
  },
  "devDependencies": {
    "@lingui/swc-plugin": "^5.5.2",
    "@lingui/vite-plugin": "^5.3.1",
    "@tailwindcss/vite": "^4.1.4",
    "@tanstack/react-query": "^5.74.7",
    "@vitejs/plugin-react-swc": "^3.9.0",
    "tailwindcss": "^4.1.4",
    "typescript": "5.8.3",
    "viem": "^2.28.0",
    "vite": "^6.3.3",
    "vitest": "^3.1.2",
    "wagmi": "^2.15.0"
  }
}
