{
  "name": "@jetstreamgg/integration-ui",
  "version": "0.1.4",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite --host --config vite.config.ts",
    "dev:restricted": "VITE_PUBLIC_RESTRICTED_BUILD=true vite --host --config vite.config.ts",
    "build": "tsc --project tsconfig.json && vite build --config vite.config.ts",
    "build:restricted": "VITE_PUBLIC_RESTRICTED_BUILD=true pnpm build",
    "preview": "pnpm build && vite preview  --config vite.config.ts",
    "preview:restricted": "VITE_PUBLIC_RESTRICTED_BUILD=true pnpm preview",
    "start": "vite --host --config vite.config.ts",
    "lint": "eslint --ext .tsx,.ts src/pages/ src/modules/",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@fontsource/plus-jakarta-sans": "^4.5.11",
    "@jetstreamgg/hooks": "workspace:^",
    "@jetstreamgg/utils": "workspace:^",
    "@jetstreamgg/widgets": "workspace:^",
    "@radix-ui/react-slot": "^1.1.1",
    "@rainbow-me/rainbowkit": "2.2.3",
    "@types/node": "18.19.74",
    "@types/react": "19.0.8",
    "@types/react-dom": "19.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.0",
    "framer-motion": "^11.18.2",
    "lucide-react": "^0.474.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^6.29.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "peerDependencies": {
    "@lingui/detect-locale": "^5.2.0",
    "@lingui/macro": "^5.2.0",
    "@lingui/react": "^5.2.0"
  },
  "devDependencies": {
    "@lingui/swc-plugin": "^5.0.2",
    "@lingui/vite-plugin": "^5.2.0",
    "@tanstack/react-query": "^5.66.0",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "tailwindcss": "^3.4.17",
    "typescript": "5.7.3",
    "viem": "^2.22.19",
    "vite": "^6.0.4",
    "vitest": "^3.0.5",
    "wagmi": "^2.14.11"
  }
}
